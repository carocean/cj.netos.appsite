{
  "name":"gbera",
  "version":"1.0",
  "title":"餐饮行业主题",
  "desc":"餐饮行业主题",
  "ctime":"2018-10-3 23:56",
  "useStyle":"blue_style",
  "developer":"cj",
  "plugin":{
    "industry":"/canyin/chuancai/shujiafengwei",
    "locality":"/zh/guangdong/guangzhou"
  },
  "displays":{
    "login_display":{
      "methods":{
        "authenticate":{
          "command":"get",
          "protocol":"http/1.1",
          "usage":"认证指定主体的身份",
          "return-type":"java.lang.String",
          "parameters":{
            "authName":{
              "type":"java.lang.String",
              "usage":"认证器名",
              "in-request":"parameter"
            },
            "tenant":{
              "type":"java.lang.String",
              "usage":"租户或租户令牌",
              "in-request":"parameter"
            },
            "principals":{
              "type":"java.lang.String",
              "usage":"要认证的主体，可以是用户名，也可是令牌(如果是令牌：结合password以认证令牌是密码持有者的）",
              "in-request":"parameter"
            },
            "password":{
              "type":"java.lang.String",
              "usage":"密码",
              "in-request":"parameter"
            },
            "ttlMillis":{
              "type":"long",
              "usage":"令牌过期时间",
              "in-request":"parameter"
            }
          }
        }
      },
      "properties":{
        "com.cj.turn":{
          "value-type":"string",
          "usage":"事件开头"
        },
        "cj.netos.set":{
          "value-type":"int",
          "usage":"好用"
        }
      }
    },
    "backdrop_display":{
      "methods":{
        "cashBillCount":{
          "command":"get",
          "protocol":"http/1.1",
          "usage":"提现单总数",
          "return-type":"double",
          "parameters":{
            "bank":{
              "type":"string",
              "usage":"银行",
              "in-request":"header"
            }
          }
        }
      },
      "properties":{
        "com.cj.turn":{
          "value-type":"string",
          "usage":"事件开头"
        },
        "cj.netos.set":{
          "value-type":"int",
          "usage":"好用"
        }
      }
    }
  },
